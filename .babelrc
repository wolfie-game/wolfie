{
  "presets": [
    [
      "@babel/preset-env",
      {
        targets: {
          esmodules: true,
        },
      },
    ], 
    "@babel/preset-react",  
    "@babel/preset-typescript"],
  "plugins": [
      ["@babel/plugin-proposal-decorators", { "legacy": true }],
      "react-hot-loader/babel",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "@loadable/babel-plugin",
    ]
}
